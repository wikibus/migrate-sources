output r2rml

map StorageLocation from wikibus.sources {
	subject template storageLocationId with Id;

	types
		wbo.StorageLocation
		hydra.Resource

	properties
		schema.containedInPlace template fileCabinetId with FileCabinet;
		dtype.position from FileOffset with datatype xsd.int;
}

map FileCabinets from wikibus.cabinets {
	subject template fileCabinetId with Id;

	types
		wbo.StorageContainer
		hydra.Resource

	properties
		rdfs.label from Description;
		wbo.size from CompartmentsCount;
}